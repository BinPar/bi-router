{
  "name": "bi-router",
  "version": "0.1.1",
  "description": "Isomorphic font-end router for next.jt, apollo-client and SEO",
  "keywords": [
    "isomorphic",
    "nodejs",
    "nextjs",
    "react",
    "seo",
    "seo-optimization",
    "apollo-client",
    "apollographql",
    "express-js",
    "graphQL",
    "GQL",
    "test",
    "apollo",
    "BinPar"
  ],
  "engines": {
    "node": ">= 6.0"
  },
  "files": ["dist"],
  "main": "./dist/bi-router",
  "scripts": {
    "lint": "eslint 'src/*' 'test/*'",
    "build": "del dist/* && babel src -d dist",
    "pretest": "npm run lint && npm run build",
    "test": "npm run testOnly",
    "testOnly": "jest --coverage",
    "testCI": "npm run test && cat coverage/lcov.info | coveralls",
    "dev": "concurrently -k 'npm run build -- -w' 'npm run testOnly -- --watch'",
    "publish": "npm run build && npm run test; npm version patch; npm publish --access public"
  },
  "author": "Ignacio Ferro PicÃ³n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BinPar/bi-router/issues"
  },
  "homepage": "https://github.com/BinPar/bi-router#readme",
  "peerDependencies": {
    "next": "4 - 5",
    "react": "16"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-transform-exponentiation-operator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-meteor": "^4.1.6",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^22.2.2",
    "next": "^5.0.0",
    "standard": "^11.0.0"
  }
}
